<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>
    <!--
    Application theme.
    <style name="AppTheme" parent="AppBaseTheme">
        All customizations that are NOT specific to a particular API-level can go here.
    </style> -->

    <style name="AppTheme" parent="AppBaseTheme">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowEnableSplitTouch">false</item>
        <item name="android:windowDisablePreview">@color/transparent</item>
        <item name="android:splitMotionEvents">false</item>
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="WelcomeActivityTheme" parent="AppBaseTheme">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowDisablePreview">true</item>
    </style>

    <style name="Translucent_whit">
        <item name="android:windowBackground">@color/transparent_white</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <!-- Note that we use the base animation style here (that is no
             animations) because we really have no idea how this kind of
             activity will be used. -->
        <item name="android:windowAnimationStyle">@android:style/Animation</item>
    </style>

    <!-- 按钮样式 -->
    <style name="but01_style">
        <item name="android:textSize">18sp</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">5dip</item>
        <item name="android:layout_marginLeft">20dip</item>
        <item name="android:layout_marginRight">20dip</item>
        <item name="android:background">@drawable/btn01_style</item>
    </style>

