    <style name="dialog_basic" parent="android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item> <!-- 边框 -->
        <item name="android:windowIsFloating">true</item> <!-- 是否浮现在activity之上 -->
        <item name="android:windowNoTitle">true</item> <!-- 无标题 -->
        　　
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:backgroundDimEnabled">true</item> <!--是否模糊-->
        <item name="android:windowBackground">@color/transparent_white</item> <!-- 背景透明 -->
    </style>

    <style name="DialogStyle" parent="@android:style/Theme.Holo.Light.Dialog.NoActionBar">
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowIsFloating">true</item><!--是否浮现在activity之上-->
        <item name="android:windowIsTranslucent">true</item><!--半透明-->
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <style name="Dialog_Clear_Data_Style" parent="@android:style/Theme.Holo.Light.Dialog.NoActionBar">
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowIsFloating">true</item><!--是否浮现在activity之上-->
        <item name="android:windowIsTranslucent">false</item><!--半透明-->
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowFrame">@null</item> <!-- 边框 -->
    </style>

    <style name="dialog_new_style" parent="@android:style/Theme.Holo.Light.Dialog.NoActionBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/transparent</item> <!--内容背景为透明-->
        <item name="android:windowIsFloating">true</item><!--是否浮现在activity之上-->
        <item name="android:windowIsTranslucent">false</item><!--半透明-->
        <item name="android:windowFrame">@null</item> <!-- 边框 -->
    </style>

    <style name="PopupAnimation" mce_bogus="1" parent="android:Animation">
