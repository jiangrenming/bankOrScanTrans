<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <Button
        android:id="@+id/gongyuexiazai"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="公钥下载" />

    <Button
        android:id="@+id/canshuxiazai"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="参数下载" />

    <Button
        android:id="@+id/dayincanshu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="打印参数" />

    <Button
        android:id="@+id/qingchuneiherizhi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="清除内核日志" />

    <Button
        android:id="@+id/tongguochuankouxiazai"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="通过串口下载" />

</LinearLayout>