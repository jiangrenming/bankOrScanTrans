<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <include layout="@layout/top_title" />

    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="1px"
        android:background="@color/gray_d0d0d0" />
    
    <RelativeLayout
        android:id="@+id/ll_reprint_js"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <ImageView
            android:id="@+id/tv1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="15dp"
            android:layout_centerVertical="true"
            android:src="@drawable/icon_reprint_js"/>

        <TextView
            android:id="@+id/tv_js"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="15dp"
            android:layout_toRightOf="@id/tv1"
            android:background="@color/transparent"
            android:text="重打印结算"
            android:layout_centerVertical="true"
            android:textSize="@dimen/pub_text_size_slightly"
            android:textColor="@color/black" />

        <ImageView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon_arrow"
            android:layout_marginRight="15dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"/>
        
        
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_below="@id/tv_js"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="15dp"
            android:background="@color/gray_cacaca" />
    </RelativeLayout>
    
    <RelativeLayout
        android:id="@+id/ll_reprint_hz"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <ImageView
            android:id="@+id/tv2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="15dp"
            android:layout_centerVertical="true"
            android:src="@drawable/icon_reprint_hz"/>

        <TextView
            android:id="@+id/tv_jyhz"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="15dp"
            android:layout_toRightOf="@id/tv2"
            android:background="@color/transparent"
            android:text="打印交易汇总"
            android:layout_centerVertical="true"
            android:textSize="@dimen/pub_text_size_slightly"
            android:textColor="@color/black" />

        <ImageView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon_arrow"
            android:layout_marginRight="15dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"/>
        
        
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_below="@id/tv_jyhz"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="15dp"
            android:background="@color/gray_cacaca" />
    </RelativeLayout>
    
    <RelativeLayout
        android:id="@+id/ll_reprint_smhz"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <ImageView
            android:id="@+id/smtv2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="15dp"
            android:layout_centerVertical="true"
            android:src="@drawable/icon_reprint_hz"/>

        <TextView
            android:id="@+id/tv_smjyhz"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="15dp"
            android:layout_toRightOf="@id/smtv2"
            android:background="@color/transparent"
            android:text="打印扫码交易汇总"
            android:layout_centerVertical="true"
            android:textSize="@dimen/pub_text_size_slightly"
            android:textColor="@color/black" />

        <ImageView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon_arrow"
            android:layout_marginRight="15dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"/>
        
        
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_below="@id/tv_smjyhz"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="15dp"
            android:background="@color/gray_cacaca" />
    </RelativeLayout>
    
    <RelativeLayout
        android:id="@+id/ll_reprint_jymx"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <ImageView
            android:id="@+id/tv3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="15dp"
            android:layout_centerVertical="true"
            android:src="@drawable/icon_reprint_jymx"/>

        <TextView
            android:id="@+id/tv_jymx"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="15dp"
            android:layout_toRightOf="@id/tv3"
            android:background="@color/transparent"
            android:text="打印交易明细"
            android:layout_centerVertical="true"
            android:textSize="@dimen/pub_text_size_slightly"
            android:textColor="@color/black" />

        <ImageView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon_arrow"
            android:layout_marginRight="15dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"/>
        
        
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_below="@id/tv_jymx"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="15dp"
            android:background="@color/gray_cacaca" />
    </RelativeLayout>
    
    <RelativeLayout
        android:id="@+id/ll_reprint_jy"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <ImageView
            android:id="@+id/tv4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="15dp"
            android:layout_centerVertical="true"
            android:src="@drawable/icon_reprint_jy"/>

        <TextView
            android:id="@+id/tv_jy"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="15dp"
            android:layout_toRightOf="@id/tv4"
            android:background="@color/transparent"
            android:text="重打印交易"
            android:layout_centerVertical="true"
            android:textSize="@dimen/pub_text_size_slightly"
            android:textColor="@color/black" />

        <ImageView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon_arrow"
            android:layout_marginRight="15dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"/>
        
        
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_below="@id/tv_jy"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="15dp"
            android:background="@color/gray_cacaca" />
    </RelativeLayout>
    
    <RelativeLayout
        android:id="@+id/ll_last"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <ImageView
            android:id="@+id/tv5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="15dp"
            android:layout_centerVertical="true"
            android:src="@drawable/icon_reprint_last"/>

        <TextView
            android:id="@+id/tv_last"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="15dp"
            android:layout_toRightOf="@id/tv5"
            android:background="@color/transparent"
            android:text="重打印上一笔"
            android:layout_centerVertical="true"
            android:textSize="@dimen/pub_text_size_slightly"
            android:textColor="@color/black" />

        <ImageView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon_arrow"
            android:layout_marginRight="15dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"/>
        
        
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_below="@id/tv_last"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="15dp"
            android:background="@color/gray_cacaca" />
    </RelativeLayout>
    
</LinearLayout>