<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.nld.cloudpos.bankline.view.TypeItemView
        android:id="@+id/tiv_local"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/menu_feature_item_bg_style"
        app:tiv_content="本地数据"
        app:tiv_leftDrawable="@drawable/icon_bankcard"/>

    <com.nld.cloudpos.bankline.view.TypeItemView
        android:id="@+id/tiv_network"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/menu_feature_item_bg_style"
        app:tiv_content="联网查询"
        app:tiv_leftDrawable="@drawable/icon_scan"/>

</LinearLayout>